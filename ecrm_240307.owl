  <owl:Class rdf:about="E87_Curation_Activity">
    <rdfs:label xml:lang="en">E87 Curation Activity</rdfs:label>
    <rdfs:label xml:lang="de">E87 Kuratorische Tätigkeit</rdfs:label>
    <rdfs:label xml:lang="fr">E87 Activité curatoriale</rdfs:label>
    <rdfs:label xml:lang="zh">E87 管理</rdfs:label>
    <skos:notation>E87</skos:notation>
