    <rdfs:label xml:lang="en">E34 Inscription</rdfs:label>
    <rdfs:label xml:lang="de">E34 Inschrift</rdfs:label>
    <rdfs:label xml:lang="el">E34 Επιγραφή</rdfs:label>
