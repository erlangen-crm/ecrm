    <rdfs:range rdf:resource="http://erlangen-crm.org/current/E99_Product_Type"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >P187 has production plan</rdfs:label>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://erlangen-crm.org/current/P187_has_production_plan"/>
