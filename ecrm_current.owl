        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="http://erlangen-crm.org/current/P145_separated"/>
        </owl:onProperty>
      </owl:Restriction>
