    >P133</skos:notation>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://erlangen-crm.org/current/P104_is_subject_to">
