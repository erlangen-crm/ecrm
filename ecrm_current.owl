    >P141i</skos:notation>
    <rdfs:range rdf:resource="http://erlangen-crm.org/current/E13_Attribute_Assignment"/>
    <rdfs:label xml:lang="en">P141 was assigned by</rdfs:label>
    <rdfs:domain rdf:resource="http://erlangen-crm.org/current/E1_CRM_Entity"/>
