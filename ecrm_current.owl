  <owl:ObjectProperty rdf:about="http://erlangen-crm.org/current/P187_has_production_plan">
    <owl:inverseOf rdf:resource="http://erlangen-crm.org/current/P187i_is_production_plan_for"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
