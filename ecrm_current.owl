    <rdfs:domain rdf:resource="http://erlangen-crm.org/current/E55_Type"/>
    <rdfs:range rdf:resource="http://erlangen-crm.org/current/E17_Type_Assignment"/>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >P42i</skos:notation>
    <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P141i_was_assigned_by"/>
