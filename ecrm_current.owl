    <rdfs:domain rdf:resource="http://erlangen-crm.org/current/E2_Temporal_Entity"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://erlangen-crm.org/current/P20_had_specific_purpose">
