    <rdfs:range rdf:resource="http://erlangen-crm.org/current/E53_Place"/>
    <rdfs:domain rdf:resource="http://erlangen-crm.org/current/E4_Period"/>
    <rdfs:label xml:lang="en">P7 took place at</rdfs:label>
  </owl:ObjectProperty>
