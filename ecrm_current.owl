    >P22</skos:notation>
    <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P14_carried_out_by"/>
    <owl:inverseOf>
      <owl:InverseFunctionalProperty rdf:about="http://erlangen-crm.org/current/P22i_acquired_title_through"/>
