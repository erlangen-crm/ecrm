  <owl:ObjectProperty rdf:about="http://erlangen-crm.org/150218/P48_has_preferred_identifier">
    <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/150218/P1_is_identified_by"/>
    <rdfs:range rdf:resource="http://erlangen-crm.org/150218/E42_Identifier"/>
