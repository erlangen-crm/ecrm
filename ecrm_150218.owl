    >P38i</skos:notation>
    <owl:inverseOf rdf:resource="http://erlangen-crm.org/150218/P38_deassigned"/>
    <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/150218/P141i_was_assigned_by"/>
    <rdfs:domain rdf:resource="http://erlangen-crm.org/150218/E42_Identifier"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://erlangen-crm.org/150218/P54i_is_current_permanent_location_of">
