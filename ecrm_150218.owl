    <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/150218/P16_used_specific_object"/>
    <rdfs:label xml:lang="en">P142 used constituent</rdfs:label>
    <rdfs:domain rdf:resource="http://erlangen-crm.org/150218/E15_Identifier_Assignment"/>
  </owl:ObjectProperty>
