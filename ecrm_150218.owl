  <owl:ObjectProperty rdf:about="http://erlangen-crm.org/150218/P13_destroyed">
    <rdfs:domain rdf:resource="http://erlangen-crm.org/150218/E6_Destruction"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://erlangen-crm.org/150218/P13i_was_destroyed_by"/>
