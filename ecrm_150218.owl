    <rdfs:label xml:lang="en">P54 is current permanent location of</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="http://erlangen-crm.org/150218/P109_has_current_or_former_curator">
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >P109</skos:notation>
    <rdfs:range rdf:resource="http://erlangen-crm.org/150218/E39_Actor"/>
